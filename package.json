{
  "name": "nexusafrik",
  "description": "NexusAfrik",
  "version": "0.0.1",
  "scripts": {
    "watch:tailwind": "postcss assets/tailwind.css -o static/index.css -w",
    "build:tailwind": "NODE_ENV=production postcss assets/tailwind.css -o static/index.css",
    "dev": "env-cmd sapper dev",
    "build": "npm run build:tailwind && sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run"
  },
  "dependencies": {
    "@tailwindcss/ui": "^0.1.3",
    "compression": "^1.7.1",
    "env-cmd": "^10.1.0",
    "fastify": "^2.13.0",
    "fastify-compress": "^2.0.1",
    "fastify-cookie": "^3.6.0",
    "fastify-env": "^1.0.1",
    "fastify-formbody": "^3.1.0",
    "fastify-helmet": "^3.0.2",
    "fastify-http-proxy": "^3.0.0",
    "fastify-jwt": "^1.3.0",
    "fastify-plugin": "^1.6.1",
    "fastify-url-data": "^2.4.0",
    "flatpickr": "^4.6.3",
    "moment": "^2.24.0",
    "polka": "next",
    "rollup-plugin-includepaths": "^0.2.3",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@fullhuman/postcss-purgecss": "^2.0.6",
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "autoprefixer": "^9.7.4",
    "cssnano": "^4.1.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.26",
    "postcss-cli": "^7.1.0",
    "postcss-import": "^12.0.1",
    "postcss-load-config": "^2.1.0",
    "postcss-nested": "^4.2.1",
    "postcss-nesting": "^7.0.1",
    "rollup": "^1.20.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^5.2.0",
    "sapper": "^0.27.0",
    "stylelint-config-recommended": "^3.0.0",
    "svelte": "^3.0.0",
    "tailwindcss": "^1.2.0"
  }
}
